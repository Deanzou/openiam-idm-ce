<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "classpath://hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="org.openiam.idm.srvc.edu.course.dto.CourseAttribute" table="COURSE_ATTRIBUTE" >
        <comment></comment>
        <id name="id" type="string">
            <column name="COURSE_ATTRIBUTE_ID" length="32" />
            <generator class="uuid" />
        </id>

        <property name="name" type="string">
            <column name="NAME" length="60" >
                <comment></comment>
            </column>
        </property>

        <property name="value" type="string">
            <column name="VALUE" length="255" >
                <comment></comment>
            </column>
        </property>
        <property name="courseId" type="string">
            <column name="COURSE_ID" length="32" >
                <comment></comment>
            </column>
        </property>


    </class>
</hibernate-mapping>
