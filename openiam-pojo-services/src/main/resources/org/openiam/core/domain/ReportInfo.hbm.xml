<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "classpath://hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="org.openiam.core.domain.ReportInfo" table="REPORT_INFO">
        <id name="id" type="string">
            <column name="REPORT_INFO_ID" length="32" />
            <generator class="uuid" />
        </id>
        <property name="reportName" type="string">
            <column name="REPORT_NAME" length="64" />
        </property>
        <property name="groovyScriptPath" type="string">
            <column name="GROOVY_SCRIPT_PATH" length="255" />
        </property>
        <property name="reportFilePath" type="string">
            <column name="REPORT_FILE_PATH" length="255" />
        </property>
        <property name="params" type="string">
            <column name="PARAMS" length="255" />
        </property>
        <property name="requiredParams" type="string">
            <column name="REQUIRED_PARAMS" length="255" />
        </property>

    </class>
</hibernate-mapping>

